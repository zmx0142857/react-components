{
  "name": "@zmx0142857/react-components",
  "version": "0.0.2",
  "type": "module",
  "description": "A few useful react components.",
  "author": "zmx0142857",
  "license": "MIT",
  "homepage": "https://github.com/zmx0142857/react-components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zmx0142857/react-components.git"
  },
  "bugs": "https://github.com/zmx0142857/react-components/issues",
  "keywords": ["react", "component"],
  "files": [
    "lib"
  ],
  "main": "./lib/main.js",
  "module": "./lib/main.js",
  "types": "./lib/main.d.ts",
  "exports": {
    ".": {
      "import": "./lib/main.js",
      "require": "./lib/main.cjs",
      "types": "./lib/main.d.ts"
    },
    "./hooks": {
      "import": "./lib/hooks.js",
      "require": "./lib/hooks.cjs",
      "types": "./lib/hooks.d.ts"
    },
    "./style.css": {
      "import": "./lib/style.css",
      "require": "./lib/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build:lib": "tsc && vite build --config vite.lib.config.ts",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.2.5",
    "@types/node": "^20.5.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "antd": "^5.8.4",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.9",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "less": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.4"
  },
  "peerDependencies": {
    "@ant-design/icons": "^5.2.5",
    "antd": "^5.8.4",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0"
  }
}
